--"sampling without replacement".
d1 $ s $ shuffle 16 "speech:5 speech:2 speech:3 speech:1 speech:4 speech:6 ~ ~ ~ ~ ~ ~ ~ ~ ~ ~"
d2 $ s $ shuffle 16 "speech:5 speech:8 speech:3 speech:7 speech:4 speech:6 ~ ~ ~ ~ ~ ~ ~ ~ ~ ~"
--"sampling with replacement".
d1 $ s $ scramble 16 "speech:5 speech:2 speech:3 speech:1 speech:4 speech:6 ~ ~ ~ ~ ~ ~ ~ ~ ~ ~"
d2 $ s $ scramble 16 "speech:5 speech:8 speech:3 speech:7 speech:4 speech:6 ~ ~ ~ ~ ~ ~ ~ ~ ~ ~"

d3 $ s "wind:1 wind:2 wind:3 wind:4 wind:5 wind:6 wind:7" # speed 0.4  # room 0.4 # size 1.5

hush


